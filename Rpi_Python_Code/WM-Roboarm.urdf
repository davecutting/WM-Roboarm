<robot name="WM-Roboarm">
	<link name="link1" />
	<link name="link2" />
	<link name="link3" />
	
	<joint name="joint1" type="continuous"/>
		<parent link="link1"/>
		<child link="link2"/>
		<origin xyz="0 0 0" rpy="0 0 0" />
		<axis xyz="0 0 0" />
		<limit effort="1000.0" lower="0.0" upper="0.548" velocity="0.5"/>
	</joint>
	
	<joint name="joint2" type="continuous"/>
		<parent link="link2"/>
		<child link="link3"/>
		<origin xyz="0 0 255" rpy="0 0 0" />
		<axis xyz="0 0 0" />
		<limit effort="1000.0" lower="0.0" upper="0.548" velocity="0.5"/>
	</joint>
</robot>
	